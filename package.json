{
  "name": "healthone",
  "description": "Health One format library for JavaScript",
  "version": "3.0.0",
  "author": "aureooms",
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-async-generator-functions"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/infoderm/healthone/issues"
  },
  "dependencies": {
    "@aureooms/js-cardinality": "^3.0.0",
    "@aureooms/js-itertools": "^5.0.1",
    "date-fns": "^2.19.0",
    "faker": "^5.4.0",
    "lodash.mergewith": "^4.6.2"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-async-generator-functions": "7.13.8",
    "@babel/polyfill": "7.10.1",
    "@babel/preset-env": "7.13.10",
    "@babel/register": "7.13.8",
    "ava": "3.15.0",
    "chardet": "^1.3.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "iconv-lite": "^0.6.2",
    "np": "7.4.0",
    "nyc": "15.1.0",
    "xo": "0.38.2",
    "fixpack": "4.0.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://infoderm.github.io/healthone",
  "keywords": [
    "Health",
    "One",
    "format",
    "parser"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/infoderm/healthone"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "xo && ava",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack"
  },
  "xo": {
    "prettier": true,
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "node/file-extension-in-import": "off",
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "doc/**"
        ],
        "env": "browser"
      }
    ]
  }
}
