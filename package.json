{
  "name": "healthone",
  "description": "Health One format library for JavaScript",
  "version": "3.0.0",
  "license": "AGPL-3.0",
  "author": "aureooms",
  "homepage": "https://infoderm.github.io/healthone",
  "repository": {
    "type": "git",
    "url": "https://github.com/infoderm/healthone"
  },
  "bugs": {
    "url": "https://github.com/infoderm/healthone/issues"
  },
  "keywords": [
    "Health",
    "One",
    "format",
    "parser"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "build-docs": "esdoc",
    "build-gh-pages": "npm run build-docs",
    "commit-msg": "commitlint --edit",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "install-hooks": "husky install",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "postinstall": "npm run install-hooks",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "release": "np",
    "test": "xo && ava"
  },
  "dependencies": {
    "@aureooms/js-cardinality": "^3.0.0",
    "@aureooms/js-itertools": "^5.0.1",
    "date-fns": "^2.19.0",
    "faker": "^5.4.0",
    "lodash.mergewith": "^4.6.2"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-async-generator-functions": "7.13.8",
    "@babel/preset-env": "7.13.10",
    "@babel/register": "7.13.8",
    "@commitlint/cli": "12.0.1",
    "@js-library/commitlint-config": "0.0.4",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.6.0",
    "chardet": "^1.3.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "husky": "5.1.3",
    "iconv-lite": "^0.6.2",
    "np": "7.4.0",
    "pinst": "2.1.6",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.7",
    "xo": "0.38.2"
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": [
            "defaults",
            "maintained node versions"
          ]
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-async-generator-functions"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      },
      "debug": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": "current node"
            }
          ],
          "babel-preset-power-assert"
        ]
      }
    }
  },
  "xo": {
    "prettier": true,
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "node/file-extension-in-import": "off",
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "doc/**"
        ],
        "env": "browser"
      }
    ]
  }
}
